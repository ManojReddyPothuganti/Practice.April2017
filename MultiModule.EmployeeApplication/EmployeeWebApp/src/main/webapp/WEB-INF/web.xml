<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>CreateEmployeeServlet</servlet-name>
		<display-name>CreateEmployeeServlet</display-name>
		<description></description>
		<servlet-class>imcs.trng.CreateEmployeeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewEmployeeServlet</servlet-name>
		<display-name>ViewEmployeeServlet</display-name>
		<description></description>
		<servlet-class>imcs.trng.ViewEmployeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateEmployeeServlet</servlet-name>
		<url-pattern>/createemployee</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewEmployeeServlet</servlet-name>
		<url-pattern>/viewemployee</url-pattern>
	</servlet-mapping>
</web-app>
